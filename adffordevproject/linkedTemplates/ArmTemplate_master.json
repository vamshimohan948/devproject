{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adffordevproject"
		},
		"LS_azure_sql_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_azure_sql'"
		},
		"ls_AzureSqlDatabase_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_AzureSqlDatabase'"
		},
		"LS_AzureKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kvfordevproject.vault.azure.net/"
		},
		"LS_azure_sql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "serverfordbdev.database.windows.net"
		},
		"LS_azure_sql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "dbfordev"
		},
		"LS_azure_sql_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "dbfordev"
		},
		"ls_AzureSqlDatabase_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "serverfordbdev.database.windows.net"
		},
		"ls_AzureSqlDatabase_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "dbfordev"
		},
		"ls_AzureSqlDatabase_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "dbfordev"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adffordevproject_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_azure_sql_password": {
						"value": "[parameters('LS_azure_sql_password')]"
					},
					"ls_AzureSqlDatabase_password": {
						"value": "[parameters('ls_AzureSqlDatabase_password')]"
					},
					"LS_AzureKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_AzureKeyVault_properties_typeProperties_baseUrl')]"
					},
					"LS_azure_sql_properties_typeProperties_server": {
						"value": "[parameters('LS_azure_sql_properties_typeProperties_server')]"
					},
					"LS_azure_sql_properties_typeProperties_database": {
						"value": "[parameters('LS_azure_sql_properties_typeProperties_database')]"
					},
					"LS_azure_sql_properties_typeProperties_userName": {
						"value": "[parameters('LS_azure_sql_properties_typeProperties_userName')]"
					},
					"ls_AzureSqlDatabase_properties_typeProperties_server": {
						"value": "[parameters('ls_AzureSqlDatabase_properties_typeProperties_server')]"
					},
					"ls_AzureSqlDatabase_properties_typeProperties_database": {
						"value": "[parameters('ls_AzureSqlDatabase_properties_typeProperties_database')]"
					},
					"ls_AzureSqlDatabase_properties_typeProperties_userName": {
						"value": "[parameters('ls_AzureSqlDatabase_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}